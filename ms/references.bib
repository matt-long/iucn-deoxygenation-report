	Doi = {10.1073/pnas.1610238114},
	File = {/Users/mclong/Dropbox/read-cube/Stock. Watson. Proc National Acad Sci. 2017.pdf},
	Issn = {0027-8424},
	Journal = {Proc. Natl. Acad. Sci. U.S.A.},
	Number = {8},
	Pages = {E1441-E1449},
	Pmid = {28115722},
	Title = {Reconciling fisheries catch and ocean productivity},
