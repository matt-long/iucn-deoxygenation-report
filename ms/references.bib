	Issn = {0930-7575},
	Journal = {Clim. Dyn.},
	Number = {3-4},
	Pages = {527-546},
	Title = {Uncertainty in climate change projections: the role of internal variability},
	Volume = {38},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s00382-010-0977-x}}

