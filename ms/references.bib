	Doi = {10.1002/2017GB005633},
	File = {/Users/mclong/Dropbox/read-cube/Landolfi. Oschlies. Global Biogeochem Cy. 2017.pdf;/Users/mclong/Dropbox/read-cube/Landolfi. Oschlies. Global Biogeochem Cy. 2017 - supplement 1.pdf},
	Issn = {1944-9224},
	Journal = {Global Biogeochem. Cycles},
	Number = {8},
	Pages = {1236-1255},
	Title = {{Oceanic nitrogen cycling and {N$_2$O} flux perturbations in the Anthropocene}},
	Volume = {31},
	Year = {2017},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/2017GB005633}}

